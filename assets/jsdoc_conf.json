{
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "include": ["./assets/js", "./tests", "./tests/", "./assets/js/"]

    },
    "plugins": [],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "opts": {
        "template": "../node_modules/ink-docstrap/template",
        "encoding": "utf8",
        "destination": "./jsdoc/",
        "recurse": true,
        "query": "value",
        "private": true,
        "lenient": true

    }
}